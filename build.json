{
  "vite": true,
  "plugins": [
    [
      "build-plugin-antd",
      {
        "disableModularImport": true,
        "themeConfig": {
          "primary-color": "#f40"
        }
      }
    ],
    [
      "build-plugin-ignore-style",
      {
        "libraryName": "@alifd/next"
      },
      {
        "libraryName": "antd"
      }
    ]
  ],
  "proxy": {
    "/api": {
      "target": "http://192.168.1.29",
      "enable": true,
      "changeOrigin": true,
      "pathRewrite": {
        "/api": ""
      }
    }
  }
}